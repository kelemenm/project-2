@model IEnumerable<Domain.cParameter>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />

<h1>Vizsgálati paraméterek</h1>

<p>
    <a href="/Parameter/Create" asp-action="Create">Create New</a>
</p>
<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Paraméter kód
            </th>
            <th>
                HUMVI leírás
            </th>
            <th>
                Saját leírás
            </th>
            <th>
                Parametrikus érték
            </th>
            <th>
                Parametrikus érték jellege
            </th>
            <th>
                Létrehozva
            </th>
            <th>
                Utolsó módosítás
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParKod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HumviLeiras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SajatLeiras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParamErtek)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ParamTip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModified)
            </td>
            <td>
                <a href="/Parameter/Edit/@item.Id" asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a href="/Parameter/Details/@item.Id" asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a href="/Parameter/Delete/@item.Id" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
