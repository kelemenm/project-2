@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<Domain.cMintavevo>

@{
    ViewData["Title"] = "Mintavevők";
}
<partial name="_ListingHeader" model="Model.Count()" />

<table class="table table-striped stickyheader">
    <thead>
        <tr>
            <th>
                Azonosító
            </th>
            <th>
                Akkreditációs szám
            </th>
            <th>
                Név
            </th>
            <th>
                Cím
            </th>
            <th>
                Érvényesség kezdete
            </th>
            <th>
                Érvényesség vége
            </th>
            <th>
                Létrehozva
            </th>
            <th>
                Utolsó módosítás
            </th>
            <th class="actioncol"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.MintavevoAzonosito)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MvAkkrSzam)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Nev)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cim)
                </td>
                <td>
                    @item.ErvKezdete.ToString("yyyy.MM.dd.")
                </td>
                <td>
                    @item.ErvVege.ToString("yyyy.MM.dd.")
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Created)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastModified)
                </td>
                <td class="actioncol">
                    <partial name="_ListingActions" model="@item.Id" />
                </td>
            </tr>
        }
    </tbody>
</table>
