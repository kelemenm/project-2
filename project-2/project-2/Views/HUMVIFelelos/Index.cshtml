@model IEnumerable<Domain.cHUMVIfelelos>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />

<h1>HUMVI felelősök</h1>

<p>
    <a href="#" onclick="showPage('createHumviFelelos')">Create New</a>
</p>
<table class="table table-striped">
    <thead>
        <tr>
            <th>
                Felelős
            </th>
            <th>
                Név
            </th>
            <th>
                Cím
            </th>
            <th>
                Létrehozva
            </th>
            <th>
                Utolsó módosítás
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Felelos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nev)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cim)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Created)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastModified)
            </td>
            <td>
                <a href="#" onclick="showPage('showHumviFelelosEdit', @item.Id)">Edit</a>
                <a href="#" onclick="showPage('showHumviFelelosDetails', @item.Id)">Details</a>
                <a href="#" onclick="showPage('showHumviFelelosDelete', @item.Id)">Delete</a>
            </td>
    </tr>
}
    </tbody>
</table>
