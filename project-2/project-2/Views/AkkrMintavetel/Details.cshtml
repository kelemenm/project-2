@model Domain.cAkkrMintavetel

@{
    ViewData["Title"] = "Details";
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />

<h1>Akkreditált mintavétel státusza</h1>

<div>
    <h4>Részletek</h4>
    <hr />
    <dl class="row">
         <dt class = "col-sm-2">
             Mintavétel státusza
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AkkrMintavetelStatusz)
        </dd>
        <dt class = "col-sm-2">
           Leírás
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Leiras)
        </dd>
        <dt class = "col-sm-2">
            Létrehozva
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Created)
        </dd>
        <dt class = "col-sm-2">
            Utolsó módosítás
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastModified)
        </dd>
    </dl>
</div>
<div>
    <a href="#" onclick="showPage('showAkkrMintavetelEdit', @Model.Id)">Edit</a>
    <a href="#" onclick="showPage('akkrMintavetel')">Back to List</a>

</div>
